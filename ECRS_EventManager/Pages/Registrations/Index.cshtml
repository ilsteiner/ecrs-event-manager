@page
@model ECRS_EventManager.Pages.Registrations.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Registration[0].FormEntryID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Registration[0].FormAdminLink)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Registration[0].FormEditLink)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Registration[0].CreatedOn)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Registration[0].SubmittedOn)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Registration[0].UpdatedOn)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Registration) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FormEntryID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FormAdminLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FormEditLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedOn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubmittedOn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedOn)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
